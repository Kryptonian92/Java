	//checks the number of values in an array that are greater than y
	public void greaterThan(){
		int[] numArr = {1,3,5,7};
		int i=0;
		int y=3;
		int count=0;
		for(i=0; i<numArr.length; i++){
			if (y>numArr[i]){
				count+=1;
			}
		}
		System.out.println("There are " + count + "numbers greater than Y");
		
	}
}
